{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "react-webpack boiler plate",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --env.ENVIRONMENT=development --open --config webpack.dev.js",
    "serve:staging": "webpack-dev-server --env.ENVIRONMENT=staging --open --config webpack.dev.js",
    "serve:prod": "webpack-dev-server --env.ENVIRONMENT=production --open --config webpack.dev.js",
    "build": "webpack --env.ENVIRONMENT=production --config webpack.prod.js",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "test": "echo \"No tests\" && exit 0"
  },
  "author": "Necoline Hubner",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "dotenv": "^8.1.0",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "terser-webpack-plugin": "^1.4.1",
    "webpack-merge": "^4.2.2"
  }
}
